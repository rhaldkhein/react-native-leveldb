{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "space-before-function-paren": 0,
        "padded-blocks": 0,
        "no-dupe-class-members": 0
    },
    "globals": {
        "beforeEach": true,
        "describe": true,
        "it": true
    }
}